  <packaging>jar</packaging>

  <name>SistemaCadastroSefaz</name>
  <url>http://maven.apache.org</url>
  
  <build>
  	<finalName>$(project.artifactId)</finalName>
	<plugins>
		<plugin>
		
		<artifactId>maven-compiler-plugin</artifactId>
		<version>3.1</version>
		<configuration>
